{
  "name": "mute-bundesliga",
  "version": "0.0.6",
  "private": true,
  "description": "Create a regular expression for muting Bundesliga matches in social media apps.",
  "license": "MIT",
  "repository": "github:mrcgrtz/mute-bundesliga",
  "author": {
    "name": "Marc GÃ¶rtz",
    "email": "git@marcgoertz.de",
    "url": "https://marcgoertz.de/"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "npx eleventy --input=src --output=dist --formats=11ty.js,njk",
    "format": "xo --fix",
    "test": "xo && c8 ava",
    "coverage": "c8 report --reporter=lcov"
  },
  "files": [
    "src/mute.js",
    "scripts/cli.mjs"
  ],
  "keywords": [
    "bundesliga",
    "mute",
    "mumble",
    "regex"
  ],
  "dependencies": {
    "meow": "^13.2.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "ava": "^6.2.0",
    "c8": "^10.1.3",
    "execa": "^9.5.2",
    "xo": "^0.59.3"
  },
  "xo": {
    "overrides": [
      {
        "files": "**/*.js",
        "rules": {
          "unicorn/prefer-module": "off"
        }
      }
    ]
  }
}
