{
  "name": "twitter-mute-bundesliga",
  "version": "0.0.6",
  "private": true,
  "description": "Create a regular expression for muting Bundesliga matches in Twitter apps (like Tweetbot).",
  "license": "MIT",
  "repository": "github:mrcgrtz/twitter-mute-bundesliga",
  "author": {
    "name": "Marc GÃ¶rtz",
    "email": "git@marcgoertz.de",
    "url": "https://marcgoertz.de/"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "npx eleventy --input=src --output=dist --formats=11ty.js,njk",
    "format": "xo --fix",
    "test": "xo && c8 ava",
    "coverage": "c8 report --reporter=lcov"
  },
  "files": [
    "src/mute.js",
    "scripts/cli.mjs"
  ],
  "keywords": [
    "bundesliga",
    "mute",
    "mumble",
    "tweetbot",
    "twitter"
  ],
  "dependencies": {
    "meow": "^11.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "ava": "^5.1.1",
    "c8": "^7.12.0",
    "execa": "^7.0.0",
    "xo": "^0.53.1"
  },
  "xo": {
    "overrides": [
      {
        "files": "**/*.js",
        "rules": {
          "unicorn/prefer-module": "off"
        }
      }
    ]
  }
}
